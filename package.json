{
  "name": "@converso-empire/conso",
  "version": "1.0.2",
  "description": "Conso - A modern multi-paradigm programming language by Converso Empire",
  "private": false,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "conso": "./cli/conso.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "cli",
    "packages/cli/bin",
    "packages/interpreter/dist",
    "packages/parser/dist",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "test": "turbo run test --parallel",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "prepublishOnly": "npm run lint && npm run test && npm run build",
    "prepack": "npm run build"
  },
  "keywords": [
    "conso",
    "converso-empire",
    "programming-language",
    "multi-paradigm",
    "compiler",
    "parser",
    "javascript",
    "typescript",
    "innovation",
    "creativity",
    "intelligence",
    "language-design",
    "code-generation",
    "ast",
    "lexer",
    "interpreter"
  ],
  "author": {
    "name": "Converso Empire",
    "url": "https://conversoempire.world/",
    "email": "info@conversoempire.world"
  },
  "contributors": [
    {
      "name": "Converso Empire",
      "url": "https://github.com/Converso-Empire"
    },
    {
      "name": "Muhammad Gohar",
      "url": "https://github.com/muhammadgohar-official"
    }
  ],
  "homepage": "https://conversoempire.world/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Converso-Empire/conso.git"
  },
  "bugs": {
    "url": "https://github.com/Converso-Empire/conso/issues"
  },
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=10.0.0"
  },
  "packageManager": "npm@10.1.0",
  "dependencies": {
    "yargs": "^17.7.2",
    "chalk": "^5.3.0"
  },
  "devDependencies": {
    "prettier": "^3.4.2",
    "turbo": "^2.3.3",
    "typescript": "^5.7.2",
    "@types/yargs": "^17.0.33"
  }
}
